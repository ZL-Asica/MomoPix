{
  "name": "momopix",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.2",
  "scripts": {
    "dev:frontend": "vite dev",
    "dev:backend": "wrangler pages dev --proxy http://localhost:5173",
    "dev": "concurrently \"pnpm dev:frontend\" \"pnpm dev:backend\"",
    "build": "pnpm clean && tsc -b && vite build",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "deploy": "pnpm build && wrangler pages deploy",
    "prepare": "husky",
    "clean": "rm -rf dist ./node_modules/.tmp/tsconfig.app.tsbuildinfo"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@marsidev/react-turnstile": "^1.1.0",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@zl-asica/react": "^0.3.10",
    "immer": "^10.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-router-dom": "^7.1.1",
    "sonner": "^1.7.1",
    "spark-md5": "^3.0.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@cloudflare/workers-types": "^4.20241230.0",
    "@eslint-react/eslint-plugin": "^1.22.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/spark-md5": "^3.0.5",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "concurrently": "^9.1.1",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.3.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-plugin-checker": "^0.8.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "wrangler": "^3.99.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
